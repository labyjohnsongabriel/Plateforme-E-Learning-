{
  "name": "elearning-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend de la plateforme e-learning Youth Computing",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run type-check && npm run lint && npm run test:run",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^7.1.0",
    "@mui/x-date-pickers": "^7.1.0",
    "axios": "^1.6.8",
    "chart.js": "^4.5.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.24",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.365.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.51.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.8.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.1",
    "react-use": "^17.5.0",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@swc/plugin-emotion": "^2.5.102",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "vite": "^5.4.20",
    "vite-plugin-pwa": "^0.19.7",
    "vitest": "^1.4.0",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "mui",
    "elearning",
    "frontend",
    "spa"
  ],
  "author": {
    "name": "Youth Computing",
    "email": "contact@youthcomputing.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/youthcomputing/elearning-platform.git"
  },
  "bugs": {
    "url": "https://github.com/youthcomputing/elearning-platform/issues"
  },
  "homepage": "https://github.com/youthcomputing/elearning-platform#readme"
}
